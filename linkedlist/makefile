a.out:linked3db.o print3d.o input3d.o save3d.o delete.o deleteall3d.o sync3db.o delmid.o
	gcc -o a.out linked3db.o print3d.o input3d.o save3d.o delete.o deleteall3d.o sync3db.o delmid.o
linked3db.o:linked3db.c
	gcc -c linked3db.c -o linked3db.o
print3d.o:print3d.c 
	gcc -c print3d.c -o print3d.o
input3d.o :input3d.c
	gcc -c input3d.c -o input3d.o
save3d.o :save3d.c
	gcc -c save3d.c -o save3d.o
delete.o :delete.c
	gcc -c delete.c -o delete.o
deleteall3d.o :deleteall3d.c
	gcc -c deleteall3d.c -o deleteall3d.o
sync3db.o :sync3db.c
	gcc -c sync3db.c -o sync3db.o
delmid.o :delmid.c 
	gcc -c delmid.c -o delmid.o



